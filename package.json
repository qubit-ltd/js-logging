{
  "name": "@qubit-ltd/logging",
  "version": "1.4.13",
  "description": "A simple JavaScript logging framework",
  "author": "Haixing Hu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-logging.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-logging/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-logging",
  "keywords": [
    "javascript",
    "logging",
    "logger",
    "decorator"
  ],
  "main": "dist/logging.min.cjs",
  "module": "dist/logging.min.mjs",
  "browser": "dist/logging.min.iife.js",
  "exports": {
    ".": {
      "require": "./dist/logging.min.cjs",
      "import": "./dist/logging.min.mjs",
      "browser": "./dist/logging.min.iife.js"
    }
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run build:dev && npm run build && npm run doc",
    "test": "jest --config jest.config.cjs",
    "lint": "eslint --config eslint.config.mjs ./src ./test",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://npm.qubit.ltd/' --access public",
    "deploy:public": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://registry.npmjs.org/' --access public",
    "deploy:all": "npm run deploy && npm publish --registry='https://registry.npmjs.org/' --access public"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@haixing_hu/clone": "^1.12.2",
    "@haixing_hu/naming-style": "^1.3.3",
    "@haixing_hu/type-detect": "^1.2.2",
    "@haixing_hu/typeinfo": "^3.1.2",
    "@haixing_hu/vue3-class-component": "^2.3.1",
    "@jest/core": "^30.1.3",
    "@qubit-ltd/eslint-config": "^2.0.0",
    "@qubit-ltd/jsdoc-theme": "^1.5.1",
    "@qubit-ltd/rollup-builder": "^1.8.12",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vue/compiler-dom": "^3.5.21",
    "@vue/compiler-sfc": "^3.5.21",
    "@vue/server-renderer": "^3.5.21",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "check-es-version": "^1.5.1",
    "core-js": "^3.45.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^6.0.0",
    "jsdoc": "^4.0.4",
    "rollup": "^4.50.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0"
  },
  "packageManager": "yarn@4.5.0"
}
