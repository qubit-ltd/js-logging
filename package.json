{
  "name": "@qubit-ltd/logging",
  "version": "1.4.6",
  "description": "A simple JavaScript logging framework",
  "author": "Haixing Hu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-logging.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-logging/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-logging",
  "keywords": [
    "javascript",
    "logging",
    "logger",
    "decorator"
  ],
  "main": "dist/logging.min.cjs",
  "module": "dist/logging.min.mjs",
  "browser": "dist/logging.min.iife.js",
  "exports": {
    ".": {
      "require": "./dist/logging.min.cjs",
      "import": "./dist/logging.min.mjs",
      "browser": "./dist/logging.min.iife.js"
    }
  },
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run build:dev && npm run build && npm run doc",
    "test": "jest",
    "lint": "eslint ./src ./test",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://npm.qubit.ltd/' --access public",
    "deploy:public": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://registry.npmjs.org/' --access public",
    "deploy:all": "npm run deploy && npm run deploy:public"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.27.0",
    "@haixing_hu/clone": "^1.12.2",
    "@haixing_hu/naming-style": "^1.3.3",
    "@haixing_hu/type-detect": "^1.2.2",
    "@haixing_hu/typeinfo": "^3.1.2",
    "@haixing_hu/vue3-class-component": "^2.3.1",
    "@jest/core": "^29.7.0",
    "@qubit-ltd/eslint-config": "^1.3.4",
    "@qubit-ltd/jsdoc-minami": "^1.5.2",
    "@qubit-ltd/rollup-builder": "^1.8.9",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vue/compiler-dom": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/server-renderer": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "check-es-version": "^1.5.1",
    "core-js": "^3.41.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.33.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^4.0.2",
    "jsdoc": "^4.0.4",
    "rollup": "^4.40.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "vue": "^3.5.13"
  },
  "packageManager": "yarn@4.5.0"
}
